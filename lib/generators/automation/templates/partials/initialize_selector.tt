<% if automation == 'watir' %>
  attr_reader :browser

  def initialize(browser)
    @browser = browser
  end
<% elsif axe? %>
  attr_reader :driver
  alias page driver

  def initialize(driver)
    @driver = driver
  end
<% else %>
  attr_reader :driver

  def initialize(driver)
    @driver = driver
  end
<% end %>